{
  "name": "add-metadata-to-vrc-photo",
  "version": "0.0.1",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "tktcorporation"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/tktcorporation/add-world-name-to-vrc-photo"
  },
  "productName": "add-metadata-to-vrc-photo",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "postinstall": "simple-git-hooks",
    "dev": "eval `dbus-launch --sh-syntax` && concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron . --disable-gpu",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder --publish never",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@tanstack/react-query": "4.35.3",
    "@trpc/client": "^10.43.0",
    "@trpc/react-query": "^10.43.0",
    "@trpc/server": "^10.43.0",
    "@vitejs/plugin-react": "^4.1.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "electron-trpc": "^0.5.2",
    "eslint-plugin-neverthrow": "^1.1.4",
    "neverthrow": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.18.0",
    "vite": "^4.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.3",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "eslint": "8.52.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.31",
    "prettier": "3.0.3",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "exports": {
    "./main": "./dist/main/index.js"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn type-check && yarn lint:fix"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
