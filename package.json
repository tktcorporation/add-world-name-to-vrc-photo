{
  "name": "add-metadata-to-vrc-photo",
  "version": "0.0.1",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "tktcorporation"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/tktcorporation/add-world-name-to-vrc-photo"
  },
  "productName": "add-metadata-to-vrc-photo",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "postinstall": "simple-git-hooks",
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder --publish never",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^1.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.4.1",
    "vite": "^2.8.6"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.2",
    "@types/node": "^20.8.3",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "electron": "^26.3.0",
    "electron-builder": "^24.6.4",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.12",
    "prettier": "2.6.0",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn type-check && yarn lint:fix"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
